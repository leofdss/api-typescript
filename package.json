{
  "name": "api-typescript",
  "version": "0.0.0",
  "scripts": {
    "start": "nodemon --config nodemon.json",
    "dev": "npm start",
    "tsc": "tsc -p tsconfig.prod.json",
    "pre-build": "node ./config/pre-build",
    "pos-build": "node ./config/pos-build",
    "build": "npm run pre-build && npm run tsc && npm run pos-build",
    "prod": "sudo docker-compose -f 'docker-compose.yml' up -d --build"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "mongodb": "^3.4.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/mongodb": "^3.3.14",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.1",
    "nodemon": "^2.0.2",
    "sucrase": "^3.11.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}

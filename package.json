    "@types/find": "^0.2.1",
    "@types/http-errors": "^1.6.3",
    "@types/jasmine": "^3.5.0",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.1",
    "@types/supertest": "^2.0.8",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.4",

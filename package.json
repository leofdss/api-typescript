{
  "name": "api-typescript",
  "version": "0.0.0",
  "scripts": {
    "start": "nodemon --config ./config/nodemon/nodemon.json",
    "dev": "npm start",
    "test": "nodemon --config ./config/nodemon/nodemon.test.json",
    "lint": "tslint --project \"tsconfig.json\"",
    "debug": "nodemon --config ./config/nodemon/nodemon.debug.json",
    "prebuild": "node ./config/build/pre-build",
    "postbuild": "node ./config/build/pos-build",
    "build": "tsc -p tsconfig.prod.json ",
    "prod": "sudo docker-compose -f 'docker-compose.yml' up -d --build"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "mongodb": "^3.4.1",
    "morgan": "^1.9.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/jasmine": "^3.5.0",
    "@types/mongodb": "^3.3.14",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.1",
    "@types/supertest": "^2.0.8",
    "jasmine": "^3.5.0",
    "nodemon": "^2.0.2",
    "sucrase": "^3.11.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}

{
  "name": "api-typescript",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./config/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "nodemon --config nodemon.json",
    "dev": "npm start",
    "prod": "sudo docker-compose -f 'docker-compose.yml' up -d --build",
    "test": "nodemon --config nodemon.test.json"
  },
  "_moduleAliases": {
    "@controllers": "dist/app/controllers",
    "@middlewares": "dist/app/middlewares",
    "@models": "dist/app/models",
    "@routes": "dist/app/routes",
    "@assets": "dist/assets",
    "@utils": "dist/utils",
    "@app": "src/app/app"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/find": "^0.2.1",
    "@types/http-errors": "^1.6.3",
    "@types/jasmine": "^3.5.0",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.1",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.5.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}
